{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist",
    "start": "node index.js",
