{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist",
    "start": "npm-run-all --parallel run restart",
    "run": "node index.js",
    "docs": "apidoc -i lib/ -o docs/ && cd docs && live-server",
    "deploy": "yarn build && git add -A . && git commit -am 'redeploy' && git push origin master",
    "restart": "sleep 300 && killall node && yarn start"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "memory-cache": "^0.1.6",
    "random-useragent": "^0.3.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  }
}
